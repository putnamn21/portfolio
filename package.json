        "cross-env": "^5.1",
        "eslint": "^4.18.1",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-react": "^7.7.0",
        "jquery": "^3.2",
        "laravel-mix": "^2.0"
    },
    "dependencies": {
        "axios": "^0.17",
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^4.0.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "three": "^0.90.0"
    }
